<table>
  <tr>
    <th>Date</th>
    <th>Miles Driven</th>
    <th>Avg MPG</th>
    <th>Operating Time (hh:mm)</th>
    <th>Idle Time (hh:mm)</th>
    <th>Speed</th>
    <th>Geofence</th>
    <th>Idle</th>
    <th>Aggressive</th>
    <th>After Hours</th>
  </tr>
  <% report.data.each do |row| -%>
    <tr>
      <td><%= row[:date] %></td>
      <td><%= row[:miles] %></td>
      <td><%= row[:mpg] %></td>
      <td><%= row[:duration] %></td>
      <td><%= row[:idle_time] %></td>
      <td><%= row[:speed] %></td>
      <td><%= row[:geofence] %></td>
      <td><%= row[:idle_time] %></td>
      <td><%= row[:aggressive] %></td>
      <td><%= row[:after_hours] %></td>
    </tr>
  <% end -%>
</table>
